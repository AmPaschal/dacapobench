         stderr digest 0x355bcc577d7183bf81865d36b24f975a3a0f8e5b;
         
size large args "simple_produce_bench-large.json", "10000000"
  output stdout digest 0xb8e8613d28ebbb8a76169e4971af73027d913fa5,
         stderr digest 0x355bcc577d7183bf81865d36b24f975a3a0f8e5b;

